<?xml version="1.0" encoding="UTF-8"?>
<conf>
	<version>0.9 beta</version>
	<langage>French</langage>
	<lookAndFeel>com.sun.java.swing.plaf.nimbus.NimbusLookAndFeel</lookAndFeel>
	<loglevel>INFO</loglevel>
	<default-website-dir>c:/</default-website-dir>
	<default-library>Library</default-library>
	<pricers>
		<pricer>
				<class>org.magic.api.pricers.impl.EbayPricer</class>
				<enable>true</enable>
		</pricer>
		<pricer>
				<class>org.magic.api.pricers.impl.MagicTradersPricer</class>
				<enable>false</enable>
		</pricer>
		<pricer>
				<class>org.magic.api.pricers.impl.MagicCardMarketPricer</class>
				<enable>true</enable>
		</pricer>
		<pricer>
				<class>org.magic.api.pricers.impl.MagicVillePricer</class>
				<enable>true</enable>
		</pricer>
		<pricer>
				<class>org.magic.api.pricers.impl.TCGPlayerPricer</class>
				<enable>false</enable>
		</pricer>
		<pricer>
				<class>org.magic.api.pricers.impl.ChannelFireballPricer</class>
				<enable>false</enable>
		</pricer>
	</pricers>
	<providers>
		<provider>
			<class>org.magic.api.providers.impl.MtgjsonProvider</class>
			<enable>true</enable>
		</provider>
		<provider>
			<class>org.magic.api.providers.impl.DeckbrewProvider</class>
			<enable>false</enable>
		</provider>
		<provider>
			<class>org.magic.api.providers.impl.MagicTheGatheringIOProvider</class>
			<enable>false</enable>
		</provider>
		<provider>
			<class>org.magic.api.providers.impl.MtgapiProvider</class>
			<enable>false</enable>
		</provider>
	</providers>
	<daos>
		<dao>
			<class>org.magic.api.dao.impl.HsqlDAO</class>
			<enable>true</enable>
		</dao>
		<dao>
			<class>org.magic.api.dao.impl.MysqlDAO</class>
			<enable>false</enable>
		</dao>
	</daos>
	<shoppers>
		<shopper>
			<class>org.magic.api.shopping.impl.LeboncoinShopper</class>
			<enable>true</enable>
		</shopper>
		<shopper>
			<class>org.magic.api.shopping.impl.EbayShopper</class>
			<enable>true</enable>
		</shopper>
		<shopper>
			<class>org.magic.api.shopping.impl.PriceMinisterShopper</class>
			<enable>true</enable>
		</shopper>
	</shoppers>
	<dashboards>
		<dashboard>
			<class>org.magic.api.dashboard.impl.MTGoldFishDashBoard</class>
			<enable>true</enable>
		</dashboard>
	</dashboards>
	<decksniffer>
		<sniffer>
			<class>org.magic.api.decksniffer.impl.TappedOutDeckSniffer</class>
			<enable>true</enable>
		</sniffer>
		<sniffer>
			<class>org.magic.api.decksniffer.impl.MTGoldFishDeck</class>
			<enable>true</enable>
		</sniffer>
		<sniffer>
			<class>org.magic.api.decksniffer.impl.MagicCorporationDecks</class>
			<enable>true</enable>
		</sniffer>
		<sniffer>
			<class>org.magic.api.decksniffer.impl.LotusNoirDecks</class>
			<enable>true</enable>
		</sniffer>
	</decksniffer>
	<deckexports>
		<export>
			<class>org.magic.api.exports.impl.CocatriceDeckExport</class>
			<enable>true</enable>
		</export>
		<export>
			<class>org.magic.api.exports.impl.CSVExport</class>
			<enable>true</enable>
		</export>
		<export>
			<class>org.magic.api.exports.impl.MKMWantList</class>
			<enable>true</enable>
		</export>
		<export>
			<class>org.magic.api.exports.impl.PDFExport</class>
			<enable>true</enable>
		</export>
		<export>
			<class>org.magic.api.exports.impl.MTGDesktopCompanionExport</class>
			<enable>true</enable>
		</export>
		<export>
			<class>org.magic.api.exports.impl.MTGODeckExport</class>
			<enable>true</enable>
		</export>
	</deckexports>
	<rss>
		<flux>
			<category>Spoil</category>
			<name>Magic Spoiler</name>
			<url>http://www.magicspoiler.com/feed/</url>
