<?xml version="1.0" encoding="UTF-8"?>
<conf>
	<langage>French</langage>
	<lookAndFeel>com.sun.java.swing.plaf.nimbus.NimbusLookAndFeel</lookAndFeel>
	<loglevel>INFO</loglevel>
	<default-website-dir>c:/</default-website-dir>
	<default-library>Library</default-library>
	<default-land-deck>KLD</default-land-deck>
	<keystore-name>jssecacerts</keystore-name>
	<keystore-pass>changeit</keystore-pass>
	<min-price-alert>0</min-price-alert>
	<debug-json-panel>false</debug-json-panel>
	<tooltip>true</tooltip>
	<modules>
		<search>true</search>
		<dashboard>true</dashboard>
		<deckbuilder>true</deckbuilder>
		<collection>true</collection>
		<game>true</game>
		<rss>false</rss>
		<alarm>true</alarm>
		<shopper>true</shopper>
		<cardbuilder>true</cardbuilder>
		<stock>true</stock>
	</modules>
	
	<game>
		<cards>
			<card-width>154</card-width>
			<card-height>215</card-height>
		</cards>
		<player-profil>
			<name>Player 1</name>
			<avatar></avatar>
			<foreground></foreground>
			<background></background>
		</player-profil>
	</game>	
	<caches>
		<cache>
			<class>org.magic.api.cache.impl.FileCache</class>
			<enable>false</enable>
		</cache>
		<cache>
			<class>org.magic.api.cache.impl.MemoryCache</class>
			<enable>false</enable>
		</cache>
		<cache>
			<class>org.magic.api.cache.impl.NoCache</class>
			<enable>true</enable>
		</cache>
	</caches>
	
	
	<servers>
		<server>
			<class>org.magic.servers.impl.ConsoleServer</class>
			<enable>false</enable>
		</server>
		<server>
			<class>org.magic.servers.impl.JSONHttpServer</class>
			<enable>false</enable>
		</server>
		<server>
			<class>org.magic.servers.impl.PricesCheckerTimer</class>
			<enable>false</enable>
		</server>
		<server>
			<class>org.magic.servers.impl.MTGGameRoomServer</class>
			<enable>false</enable>
		</server>
	</servers>

	<pricers>
		<pricer>
				<class>org.magic.api.pricers.impl.EbayPricer</class>
				<enable>true</enable>
		</pricer>
		<pricer>
				<class>org.magic.api.pricers.impl.MagicCardMarketPricer2</class>
				<enable>true</enable>
		</pricer>
		<pricer>
				<class>org.magic.api.pricers.impl.MagicTradersPricer</class>
				<enable>false</enable>
		</pricer>
		<pricer>
				<class>org.magic.api.pricers.impl.MagicVillePricer</class>
				<enable>true</enable>
		</pricer>
		<pricer>
				<class>org.magic.api.pricers.impl.TCGPlayerPricer</class>
				<enable>false</enable>
		</pricer>
		<pricer>
				<class>org.magic.api.pricers.impl.ChannelFireballPricer</class>
				<enable>false</enable>
		</pricer>
		<pricer>
				<class>org.magic.api.pricers.impl.CardKingdomPricer</class>
				<enable>false</enable>
		</pricer>
	</pricers>
	<providers>
		<provider>
			<class>org.magic.api.providers.impl.MtgjsonProvider</class>
			<enable>true</enable>
		</provider>
		<provider>
			<class>org.magic.api.providers.impl.DeckbrewProvider</class>
			<enable>false</enable>
		</provider>
		<provider>
			<class>org.magic.api.providers.impl.MagicTheGatheringIOProvider</class>
			<enable>false</enable>
		</provider>
		<provider>
			<class>org.magic.api.providers.impl.MtgapiProvider</class>
			<enable>false</enable>
		</provider>
	</providers>
	<daos>
		<dao>
			<class>org.magic.api.dao.impl.HsqlDAO</class>
			<enable>true</enable>
		</dao>
		<dao>
			<class>org.magic.api.dao.impl.MysqlDAO</class>
			<enable>false</enable>
		</dao>
		<dao>
			<class>org.magic.api.dao.impl.PostgresqlDAO</class>
			<enable>false</enable>
		</dao>
		<dao>
			<class>org.magic.api.dao.impl.FileDAO</class>
			<enable>false</enable>
		</dao>
	</daos>
	<shoppers>
		<shopper>
			<class>org.magic.api.shopping.impl.LeboncoinShopper</class>
			<enable>true</enable>
		</shopper>
		<shopper>
			<class>org.magic.api.shopping.impl.EbayShopper</class>
			<enable>true</enable>
		</shopper>
		<shopper>
			<class>org.magic.api.shopping.impl.PriceMinisterShopper</class>
			<enable>true</enable>
		</shopper>
	</shoppers>
	<dashboards>
		<dashboard>
			<class>org.magic.api.dashboard.impl.MTGoldFishDashBoard</class>
			<enable>true</enable>
		</dashboard>
		<dashboard>
			<class>org.magic.api.dashboard.impl.MTGPriceDashBoard</class>
			<enable>false</enable>
		</dashboard>
	</dashboards>
	<pictures>
		<picture>
			<class>org.magic.api.pictures.impl.GathererPicturesProvider</class>
			<enable>true</enable>
		</picture>
		<picture>
			<class>org.magic.api.pictures.impl.MagicCardInfoPicturesProvider</class>
			<enable>false</enable>
		</picture>
		<picture>
			<class>org.magic.api.pictures.impl.DeckMasterPicturesProvider</class>
			<enable>true</enable>
		</picture>
		<picture>
			<class>org.magic.api.pictures.impl.PersonalSetPicturesProvider</class>
			<enable>true</enable>
		</picture>
	</pictures>
	<decksniffer>
		<sniffer>
			<class>org.magic.api.decksniffer.impl.TappedOutDeckSniffer</class>
			<enable>true</enable>
		</sniffer>
		<sniffer>
			<class>org.magic.api.decksniffer.impl.MTGoldFishDeck</class>
			<enable>true</enable>
		</sniffer>
		<sniffer>
			<class>org.magic.api.decksniffer.impl.TCGPlayerDeckSniffer</class>
			<enable>false</enable>
		</sniffer>
		<sniffer>
			<class>org.magic.api.decksniffer.impl.MagicCorporationDecks</class>
			<enable>false</enable>
		</sniffer>
		<sniffer>
			<class>org.magic.api.decksniffer.impl.LotusNoirDecks</class>
			<enable>true</enable>
		</sniffer>
	</decksniffer>
	<deckexports>
		<export>
			<class>org.magic.api.exports.impl.CocatriceDeckExport</class>
			<enable>true</enable>
		</export>
		<export>
			<class>org.magic.api.exports.impl.CSVExport</class>
			<enable>true</enable>
		</export>
		<export>
			<class>org.magic.api.exports.impl.MKMFileWantListExport</class>
			<enable>true</enable>
		</export>
		<export>
			<class>org.magic.api.exports.impl.MkmOnlineExport</class>
			<enable>true</enable>
		</export>
		<export>
			<class>org.magic.api.exports.impl.PDFExport</class>
			<enable>true</enable>
		</export>
		<export>
			<class>org.magic.api.exports.impl.MTGDesktopCompanionExport</class>
			<enable>true</enable>
		</export>
		<export>
			<class>org.magic.api.exports.impl.MTGODeckExport</class>
			<enable>true</enable>
		</export>
		<export>
			<class>org.magic.api.exports.impl.JsonExport</class>
			<enable>true</enable>
		</export>
		<export>
			<class>org.magic.api.exports.impl.DCIDeckSheetExport</class>
			<enable>true</enable>
		</export>
		<export>
			<class>org.magic.api.exports.impl.Apprentice2DeckExport</class>
			<enable>true</enable>
		</export>
	</deckexports>
	<rss>
		<flux>
			<category>Spoil</category>
			<name>Magic Spoiler</name>
